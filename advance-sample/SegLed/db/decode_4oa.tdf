--synthesis_resources = lut 4 
SUBDESIGN decode_4oa
( 
	data[1..0]	:	input;
	enable	:	input;
	eq[3..0]	:	output;
) 
VARIABLE 
	data_wire[1..0]	: WIRE;
	enable_wire	: WIRE;
	eq_node[3..0]	: WIRE;
	eq_wire[3..0]	: WIRE;
	w_anode841w[2..0]	: WIRE;
	w_anode854w[2..0]	: WIRE;
	w_anode862w[2..0]	: WIRE;
	w_anode870w[2..0]	: WIRE;

BEGIN 
	data_wire[] = data[];
	enable_wire = enable;
	eq[] = eq_node[];
	eq_node[3..0] = eq_wire[3..0];
	eq_wire[] = ( w_anode870w[2..2], w_anode862w[2..2], w_anode854w[2..2], w_anode841w[2..2]);
	w_anode841w[] = ( (w_anode841w[1..1] & (! data_wire[1..1])), (w_anode841w[0..0] & (! data_wire[0..0])), enable_wire);
	w_anode854w[] = ( (w_anode854w[1..1] & (! data_wire[1..1])), (w_anode854w[0..0] & data_wire[0..0]), enable_wire);
	w_anode862w[] = ( (w_anode862w[1..1] & data_wire[1..1]), (w_anode862w[0..0] & (! data_wire[0..0])), enable_wire);
	w_anode870w[] = ( (w_anode870w[1..1] & data_wire[1..1]), (w_anode870w[0..0] & data_wire[0..0]), enable_wire);
END;
